}

def equalZero (x) {
    
    def positive_mod (a, b) {
        res = a - ((a // b) * b)
    }
    
    def positive_floor (x) {
        res = x - positive_mod(x, 1)
    }
    
    def function (x) {
        res = abs(x) / (abs(x) +1)
    }
    
    res = positive_floor(1 - function(x))
}


